# YAHEALTHY Backend Configuration

# Server
PORT=5000
NODE_ENV=development

# Authentication
JWT_SECRET=your-super-secret-key-change-in-production

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-public-key

# AI Provider (Optional - for real food photo estimation)
AI_PROVIDER=google-vision
AI_API_KEY=your-google-vision-api-key
AI_ENDPOINT=https://vision.googleapis.com/v1/images:annotate

# Places Provider (Optional - for real restaurant suggestions)
PLACES_PROVIDER=google-places
PLACES_API_KEY=your-google-places-api-key
PLACES_ENDPOINT=https://maps.googleapis.com/maps/api/place/nearbysearch/json

# Database
DB_POOL_SIZE=10
DB_TIMEOUT=30000
